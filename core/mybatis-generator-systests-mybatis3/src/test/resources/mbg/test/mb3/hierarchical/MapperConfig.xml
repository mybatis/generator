    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/propertyType/withLobs/sqlXmlMappings/UserBlogMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/propertyType/withLobs/sqlXmlMappings/UserPhotosMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/propertyType/withLobs/sqlXmlMappings/UserSkillsMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/propertyType/withLobs/sqlXmlMappings/UsersMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/propertyType/withLobs/sqlXmlMappings/UsersToSkillsMapper.xml" />
