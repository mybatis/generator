<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

  <settings>
    <setting name="cacheEnabled" value="true"/>
    <setting name="lazyLoadingEnabled" value="false"/>
    <setting name="multipleResultSetsEnabled" value="true"/>
    <setting name="useColumnLabel" value="true"/>
    <setting name="defaultExecutorType" value="SIMPLE"/>
    <setting name="defaultStatementTimeout" value="25000"/>
  </settings>

  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC">
        <property name="" value=""/>
      </transactionManager>
      <dataSource type="UNPOOLED">
        <property name="driver" value="org.hsqldb.jdbcDriver"/>
        <property name="url" value="jdbc:hsqldb:mem:aname"/>
        <property name="username" value="sa"/>
      </dataSource>
    </environment>
  </environments>

  <mappers>
    <mapper resource="mbg/test/mb3/generated/hierarchical/xml/AwfulTableMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/xml/FieldsblobsMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/xml/subpackage/FieldsonlyMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/xml/PkblobsMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/xml/PkfieldsMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/xml/PkfieldsblobsMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/xml/PkonlyMapper.xml" />

    <!-- Jaxb Annotations plugin specific mappers - For Hierarchical Model-->

    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/fieldType/noLobs/sqlXmlMappings/UserBlogMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/fieldType/noLobs/sqlXmlMappings/UserPhotosMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/fieldType/noLobs/sqlXmlMappings/UserSkillsMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/fieldType/noLobs/sqlXmlMappings/UsersMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/fieldType/noLobs/sqlXmlMappings/UsersToSkillsMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/fieldType/noLobs/sqlXmlMappings/UserTutorialMapper.xml" />

    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/fieldType/withLobs/sqlXmlMappings/UserBlogMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/fieldType/withLobs/sqlXmlMappings/UserPhotosMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/fieldType/withLobs/sqlXmlMappings/UserSkillsMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/fieldType/withLobs/sqlXmlMappings/UsersMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/fieldType/withLobs/sqlXmlMappings/UsersToSkillsMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/fieldType/withLobs/sqlXmlMappings/UserTutorialMapper.xml" />

    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/propertyType/noLobs/sqlXmlMappings/UserBlogMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/propertyType/noLobs/sqlXmlMappings/UserPhotosMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/propertyType/noLobs/sqlXmlMappings/UserSkillsMapper.xml" />
    <mapper resource="mbg/test/mb3/generated/hierarchical/plugins/jaxbAnnotations/propertyType/noLobs/sqlXmlMappings/UsersMapper.xml" />
