<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig
  PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
  "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>

  <settings useStatementNamespaces="true" />

  <transactionManager type="JDBC">
    <dataSource type="SIMPLE">
      <property name="JDBC.Driver" value="org.hsqldb.jdbcDriver"/>
      <property name="JDBC.ConnectionURL" value="jdbc:hsqldb:mem:aname"/>
      <property name="JDBC.Username" value="sa"/>
      <property name="JDBC.Password" value=""/>
      <property name="JDBC.DefaultAutoCommit" value="true"/>
    </dataSource>
  </transactionManager>

  <sqlMap resource="ibatortest/java2/generated/flat/java2/xml/FIELDSBLOBS_SqlMap.xml" />
  <sqlMap resource="ibatortest/java2/generated/flat/java2/xml/FIELDSONLY_SqlMap.xml" />
  <sqlMap resource="ibatortest/java2/generated/flat/java2/xml/PKBLOBS_SqlMap.xml" />
  <sqlMap resource="ibatortest/java2/generated/flat/java2/xml/PKFIELDS_SqlMap.xml" />
  <sqlMap resource="ibatortest/java2/generated/flat/java2/xml/PKFIELDSBLOBS_SqlMap.xml" />
  <sqlMap resource="ibatortest/java2/generated/flat/java2/xml/PKONLY_SqlMap.xml" />
  <sqlMap resource="ibatortest/java2/generated/flat/java2/xml/awful table_SqlMap.xml" />

</sqlMapConfig>
